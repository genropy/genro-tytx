{
  "name": "genro-tytx",
  "version": "0.7.0",
  "description": "TYTX Base - Minimal Typed Text Protocol for Scalar Types (TypeScript)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
