{
  "name": "genro-tytx-ts",
  "version": "0.2.5",
  "description": "TYTX (Typed Text) - TypeScript implementation with full type safety",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc",
    "test": "node --experimental-vm-modules node_modules/vitest/vitest.mjs run",
    "test:watch": "node --experimental-vm-modules node_modules/vitest/vitest.mjs",
    "coverage": "node --experimental-vm-modules node_modules/vitest/vitest.mjs run --coverage",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@msgpack/msgpack": "^3.1.2",
    "@types/node": "^20.0.0",
    "@vitest/coverage-v8": "^2.1.9",
    "big.js": "^6.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@msgpack/msgpack": ">=3.0.0",
    "big.js": ">=6.0.0"
  },
  "peerDependenciesMeta": {
    "@msgpack/msgpack": {
      "optional": true
    },
    "big.js": {
      "optional": true
    }
  },
  "keywords": [
    "typed",
    "serialization",
    "protocol",
    "json",
    "xml",
    "msgpack",
    "genro",
    "tytx",
    "typescript"
  ],
  "author": "Genropy Team",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genropy/genro-tytx.git",
    "directory": "ts"
  },
  "bugs": {
    "url": "https://github.com/genropy/genro-tytx/issues"
  },
  "homepage": "https://github.com/genropy/genro-tytx#readme",
  "engines": {
    "node": ">=18.0.0"
  }
}
