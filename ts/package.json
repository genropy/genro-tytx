  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/genropy/genro-tytx.git",
    "directory": "ts"
  },
  "bugs": {
    "url": "https://github.com/genropy/genro-tytx/issues"
  },
  "homepage": "https://github.com/genropy/genro-tytx#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "big.js": ">=6.0.0",
